<p id="notice"><%= notice %></p>

<h1>Games</h1>

<table>
  <thead>
    <tr>
      <th>Round</th>
      <th>Team</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @games.each do |game| %>
      <tr>
        <td><%= game.round %></td>
        <td><%= game.team %></td>
        <td><%= game.score %></td>
        <td><%= link_to 'Show', game %></td>
        <td><%= link_to 'Edit', edit_game_path(game) %></td>
        <td><%= link_to 'Destroy', game, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game', new_game_path %>

<html>
<head>
<script type="text/javascript" src="script.js"></script>
<link rel="stylesheet" type="text/css" href="style.css"> 
</head>
<body>
<table style="height: 423px;" width="738">
<tbody>
<tr>
<td style="text-align: center; vertical-align: middle;">    

<div class="boxb">    
        <label for="BLUE"><abbr title="Qty">BLUE</abbr></label>
        <input id="qtyblue" value="0" />
        <button id="down" onclick="modify_qtyblue(-1)">-1</button>
        <button id="up"  accesskey="d" onclick="modify_qtyblue(1)">+1</button>
</div>

</td>
<td style="text-align: center; vertical-align: middle;">    

<div class="boxr">    
        <label for="RED"><abbr title="Qty">RED</abbr></label>
        <input id="qtyred" value="0" />
        <button id="down" onclick="modify_qtyred(-1)">-1</button>
        <button id="up" accesskey="k" onclick="modify_qtyred(1)">+1</button>
</div>

</td>
</tr>
</tbody>
</table>
</body>
</html>






